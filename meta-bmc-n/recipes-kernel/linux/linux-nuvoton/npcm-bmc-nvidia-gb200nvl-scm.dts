		"HMC_PRSNT_L-I",
		"HMC_READY-I",
		"HMC_EROT_FATAL_ERROR_L-I",
		"I2C_MUX_SEL-O",
		"HMC_EROT_SPI_MUX_SEL-O",
		"HMC_EROT_RECOVERY_L-O",
		"HMC_EROT_RST_L-O",
		"GLOBAL_WP_HMC-O",
		"FPGA_RST_L-O",
		"USB2_HUB_RST-O",
		"CPU_UART_MUX_SEL-O",
		"",
		"",
		"",
		"",
		"";
	};

	// HMC Expander @0x74
	exp3: pca9555@74 {
		compatible = "nxp,pca9555";
		reg = <0x74>;
		gpio-controller;
		#gpio-cells = <2>;
