		pulse_width_us = <5>;
		timeout_ms = <4995>;
		reg = <0x10>;
	};
};

&i2c15 {
	status = "okay";
	clock-frequency = <400000>;
	multi-master;
};

&i2c16 {
	status = "okay";
	clock-frequency = <100000>;
	multi-master;
