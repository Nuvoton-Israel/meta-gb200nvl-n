	status = "okay";
	// Module 0, Expander @0x20
	exp0: pca9555@20 {
		compatible = "nxp,pca9555";
		reg = <0x20>;
		gpio-controller;
		#gpio-cells = <2>;
		#address-cells = <1>;
		#size-cells = <0>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupt-parent = <&gpio3>;
		interrupts = <31 IRQ_TYPE_EDGE_FALLING>; /* gpio127 */

		gpio-line-names =
		"FPGA_THERM_OVERT_L-I",
		"FPGA_READY_BMC-I",
		"HMC_BMC_DETECT-O",
		"HMC_PGOOD-O",
