		"BMC_EROT_FPGA_SPI_MUX_SEL-O",
		"USB_HUB_RESET_L-O",
		"NCSI_CS1_SEL-O",
		"SGPIO_EN_L-O",
		"B2B_IOEXP_INT_L-I",
		"I2C_BUS_MUX_RESET_L-O";
	};

