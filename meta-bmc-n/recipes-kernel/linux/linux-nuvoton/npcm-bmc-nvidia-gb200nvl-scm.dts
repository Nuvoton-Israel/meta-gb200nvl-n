/ {
	model = "NPCM845 GB200NVL DC-SCM";
	compatible = "nuvoton,npcm845";

	aliases {
		serial0 = &serial0;
		serial1 = &serial1;
		serial2 = &serial3;
		serial3 = &serial4;
		ethernet0 = &gmac0;
		ethernet2 = &gmac2;
		fiu0 = &fiu0;
		fiu1 = &fiu1;
		fiu3 = &fiu3;
		i2c0 = &i2c14;
		i2c1 = &i2c15;
		i2c2 = &i2c20;
		i2c3 = &i2c23;
		i2c4 = &i2c24;
		i2c5 = &i2c25;
		i2c6 = &i2c26;
		i2c7 = &i2c2;
		i2c8 = &i2c3;
		i2c9 = &i2c6;
		i2c10 = &i2c5;
		i2c11 = &i2c13;
		i2c12 = &i2c7;
		i2c13 = &i2c0;
		i2c14 = &i2c16;
		i2c15 = &i2c12;
		i2c16 = &imux16;
		i2c17 = &imux17;
		i2c18 = &imux18;
		i2c19 = &imux19;
		i2c20 = &imux20;
		i2c21 = &imux21;
		i2c22 = &imux22;
		i2c23 = &imux23;
		i2c60 = &i2c1;
		i2c61 = &i2c11;
	};

	chosen {
		stdout-path = &serial0;
	};

	memory {
		reg = <0x0 0x0 0x0 0x40000000>;
	};

	led_bmc_hb {
		compatible = "gpio-leds";
		heartbeat {
			label = "heartbeat";
			gpios = <&gpio2 19 GPIO_ACTIVE_LOW>; /* gpio83 */
			linux,default-trigger = "timer";
			led-pattern = <1000 1000>;
			default-state = "on";
		};
	};

	leds {
		compatible = "gpio-leds";
		uid_led {
			gpios = <&siox_gpio 27 GPIO_ACTIVE_LOW>;
		};
		fault_led {
			gpios = <&siox_gpio 29 GPIO_ACTIVE_LOW>;
		};
		power_led {
			gpios = <&siox_gpio 31 GPIO_ACTIVE_LOW>;
		};

		// Non-LEDs:
		//   BMC_READY-O GPIO pin (not an LED) is being bound to the GPIO LED driver.
		// Notes:
		// * This is a workaround and leverages the GPIO LED driver to enable control of
		//   reset tolerance and still allow the GPIO to be controlled from user space.
		// * The standard Linux GPIO driver allows control of reset tolerance, however
		//   does not expose user space APIs for user space control of the GPIO pin.
		// * GPIO_TRANSITORY = reset tolerance is disabled
		// * Any non-leds should be added below this line.
		bmc_ready_noled {
			gpios = <&gpio5 27 GPIO_ACTIVE_HIGH>; /* GPIO187 */
		};
	};

	refclk: refclk-25mhz {
		compatible = "fixed-clock";
		clock-output-names = "ref";
		clock-frequency = <25000000>;
		#clock-cells = <0>;
	};

	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		tip_reserved: tip@0x0 {
			reg = <0x0 0x0 0x0 0x6200000>;
		};
	};
};

&gmac0 {
	phy-mode = "sgmii";
	npcm,sgmii-an;
	snps,eee-force-disable;
	status = "okay";
};

&gmac2 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&r1_pins
		     &r1oen_pins>;
	use-ncsi;
};

