&i2c11 {
	status = "okay";
	//Internal eeprom for DVO
	slave_eeprom: slave_eeprom@40000050 {
		compatible = "slave-24c02";
		reg = <0x40000050>;
		status = "okay";
	};
};

&i2c12 {
	status = "okay";
	clock-frequency = <100000>;
	multi-master;

	//E1.S drive slot 4-7
	i2c-switch@77 {
		compatible = "nxp,pca9546";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x77>;
		i2c-mux-idle-disconnect;

		e1si2c4: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
		};

		e1si2c5: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
		};

		e1si2c6: i2c@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;
		};

		e1si2c7: i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;
		};
	};
};

&i2c14 {
	status = "okay";
	clock-frequency = <400000>;
	ssif-bmc@10 {
		compatible = "ssif-bmc";
		alert-gpio = <&sgpio1 39 GPIO_ACTIVE_LOW>;
		pulse_width_us = <5>;
		timeout_ms = <4995>;
		reg = <0x10>;
	};
};

&i2c15 {
	status = "okay";
	clock-frequency = <400000>;
	multi-master;
};

&i2c16 {
	status = "okay";
	clock-frequency = <100000>;
	multi-master;

	//E1.S drive slot 0-3
