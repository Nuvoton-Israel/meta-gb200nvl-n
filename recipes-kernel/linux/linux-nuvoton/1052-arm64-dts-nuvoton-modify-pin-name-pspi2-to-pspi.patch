From b4adf61a895f6ee2b6d254f93adb589a364ff370 Mon Sep 17 00:00:00 2001
From: Tomer Maimon <tmaimon77@gmail.com>
Date: Tue, 19 Mar 2024 20:30:19 +0200
Subject: [PATCH 03/23] arm64: dts: nuvoton: modify pin name pspi2 to pspi

Signed-off-by: Tomer Maimon <tmaimon77@gmail.com>
---
 arch/arm64/boot/dts/nuvoton/nuvoton-common-npcm8xx.dtsi | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm64/boot/dts/nuvoton/nuvoton-common-npcm8xx.dtsi b/arch/arm64/boot/dts/nuvoton/nuvoton-common-npcm8xx.dtsi
index c8a17ee1bd22..0838eca6261c 100644
--- a/arch/arm64/boot/dts/nuvoton/nuvoton-common-npcm8xx.dtsi
+++ b/arch/arm64/boot/dts/nuvoton/nuvoton-common-npcm8xx.dtsi
@@ -271,7 +271,7 @@ spi1: spi@201000 {
 				compatible = "nuvoton,npcm845-pspi";
 				reg = <0x201000 0x1000>;
 				pinctrl-names = "default";
-				pinctrl-0 = <&pspi2_pins>;
+				pinctrl-0 = <&pspi_pins>;
 				#address-cells = <1>;
 				#size-cells = <0>;
 				interrupts = <GIC_SPI 28 IRQ_TYPE_LEVEL_HIGH>;
@@ -920,9 +920,9 @@ lkgpo0_pins: lkgpo0-pins {
 			groups = "lkgpo0";
 			function = "lkgpo0";
 		};
-		pspi2_pins: pspi2-pins {
-			groups = "pspi2";
-			function = "pspi2";
+		pspi_pins: pspi-pins {
+			groups = "pspi";
+			function = "pspi";
 		};
 		jm1_pins: jm1-pins {
 			groups = "jm1";
-- 
