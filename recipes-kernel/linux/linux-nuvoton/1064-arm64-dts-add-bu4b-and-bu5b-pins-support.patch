+++ b/arch/arm64/boot/dts/nuvoton/nuvoton-common-npcm8xx.dtsi
@@ -1867,10 +1867,18 @@ bu4_pins: bu4-pins {
 			groups = "bu4";
 			function = "bu4";
 		};
+		bu4b_pins: bu4b-pins {
+			groups = "bu4b";
+			function = "bu4b";
+		};
 		bu5_pins: bu5-pins {
 			groups = "bu5";
 			function = "bu5";
 		};
+		bu5b_pins: bu5b-pins {
+			groups = "bu5b";
+			function = "bu5b";
+		};
 		bu6_pins: bu6-pins {
 			groups = "bu6";
 			function = "bu6";
