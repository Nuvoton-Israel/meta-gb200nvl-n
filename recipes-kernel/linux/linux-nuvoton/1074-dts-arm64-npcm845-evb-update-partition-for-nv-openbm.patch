+			dummy@4000000 {
+				label = "dummy";
+				reg = <0x4000000 0x1000000>;
+				};
+			rwfs@4000000 {
 				label = "rwfs";
-				reg = <0x3f00000 0x0100000>;
+				reg = <0x4000000 0x1000000>;
+				};
+			log@4000000 {
+				label = "log";
+				reg = <0x5000000 0x2000000>;
